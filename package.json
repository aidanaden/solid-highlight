{
  "name": "solid-highlight",
  "version": "0.1.5",
  "description": "Solid component for syntax highlighting, based on react-highlight and highlightjs' vue plugin",
  "author": {
    "name": "Aidan Aden",
    "email": "vroomvroom@mailbox.org",
    "url": "https://github.com/aidanaden"
  },
  "license": "MIT",
  "type": "module",
  "source": "src/index.tsx",
  "main": "dist/highlightjs-solid.min.js",
  "module": "dist/highlightjs-solid.esm.min.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "solid": "./dist/source/index.jsx",
      "require": "./dist/highlightjs-solid.min.js",
      "import": "./dist/highlightjs-solid.esm.min.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "sideEffects": false,
  "engines": {
    "node": ">=10",
    "yarn": ">=1.22.5"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "cd example && vite",
    "build": "rollup -c",
    "prepublishOnly": "yarn build",
    "serve": "vite preview"
  },
  "devDependencies": {
    "rollup": "^2.75.7",
    "rollup-preset-solid": "^1.4.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vite-plugin-solid": "^2.2.6"
  },
  "dependencies": {
    "highlight.js": "^11.5.1",
    "rollup-plugin-typescript": "^1.0.1",
    "solid-js": "^1.4.2"
  },
  "peerDependencies": {
    "highlight.js": "^11.5.1",
    "solid-js": "^1.4.2"
  }
}
