{
  "name": "solid-highlight",
  "version": "0.1.12",
  "description": "Solid component for syntax highlighting, based on react-highlight and highlightjs' vue plugin",
  "author": {
    "name": "Aidan Aden",
    "email": "vroomvroom@mailbox.org",
    "url": "https://github.com/aidanaden"
  },
  "license": "MIT",
  "type": "module",
  "source": "src/index.tsx",
  "main": "dist/highlightjs-solid.min.js",
  "module": "dist/highlightjs-solid.esm.min.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/highlightjs-solid.min.js",
      "import": "./dist/highlightjs-solid.esm.min.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "sideEffects": false,
  "engines": {
    "node": ">=10",
    "yarn": ">=1.22.5"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "cd example && vite",
    "build": "rollup -c",
    "prepublishOnly": "yarn build",
    "serve": "vite preview"
  },
  "devDependencies": {
    "rollup": "3.10.1",
    "rollup-plugin-typescript2": "0.34.1",
    "rollup-preset-solid": "1.4.0",
    "typescript": "4.8.4",
    "vite": "3.1.8",
    "vite-plugin-solid": "2.3.9"
  },
  "dependencies": {
    "highlight.js": "^11.6.0",
    "solid-js": "^1.4.8"
  },
  "keywords": [
    "solidjs component",
    "solid-component",
    "solid",
    "highlight.js",
    "syntax",
    "highlighting",
    "highlight",
    "code",
    "syntax",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aidanaden/solid-highlight.git"
  }
}
